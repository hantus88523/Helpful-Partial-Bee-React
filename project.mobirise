ata-app-selector=\".mbr-timeline-title\">\n                            <b>Form Builder</b>\n                        </h4>\n                        <p class=\"mbr-text mbr-fonts-style mt-3 mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\">\n                            This is a timeline block. Click on the blue gear icon to set the number of items in this timeline. Use Mobirise website building software to create multiple sites for commercial and non-profit projects.\n                        </p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    \n\n\n</section>\n\n<div class=\"button-container\">\n<button id=\"show-more-btn\">Show More</button>\n<button id=\"show-less-btn\" style=\"display:none;\">Show Less</button>\n</div>\n\n</div>\n\n\n\n\n<script>\nconst timelineSection = document.querySelector('.timeline3');\nconst timelineElements = timelineSection.querySelectorAll('.timeline-element');\nconst showMoreBtn = document.querySelector('#show-more-btn');\nconst showLessBtn = document.querySelector('#show-less-btn');\n\n// Store the current scroll position\nconst initialScrollPosition = window.pageYOffset;\n\n// Set initial height to 500px\ntimelineSection.style.maxHeight = \"1300px\";\n\n// Hide the \"Show Less\" button initially\nshowLessBtn.style.display = \"none\";\n\n// Add click event listener to the \"Show More\" button\nshowMoreBtn.addEventListener('click', () => {\n  // Set max height to allow all elements to be visible\n  timelineSection.style.maxHeight = \"none\";\n  // Hide the \"Show More\" button and show the \"Show Less\" button\n  showMoreBtn.style.display = \"none\";\n  showLessBtn.style.display = \"inline-block\";\n});\n\n// Add click event listener to the \"Show Less\" button\nshowLessBtn.addEventListener('click', () => {\n  // Set max height back to 500px\n  timelineSection.style.maxHeight = \"1300px\";\n  // Hide the \"Show Less\" button and show the \"Show More\" button\n  showLessBtn.style.display = \"none\";\n  showMoreBtn.style.display = \"inline-block\";\n  // Scroll back to the original position\n  const timelineSectionRect = timelineSection.getBoundingClientRect();\n  window.scrollBy(0, timelineSectionRect.top);\n});\n</script>",
