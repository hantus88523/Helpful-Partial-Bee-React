    "global_header_custom": "    <style>\n      /* CSS code for the loader */\n      .lds-circle {\n        display: inline-block;\n        position: fixed;\n        top: 50%;\n        left: 50%;\n        transform: translate(-50%, -50%) translateZ(1px);\n        z-index: 9999;\n        opacity: 1;\n        transition: opacity 0.5s ease-in-out;\n      }\n      \n      .lds-circle.fade-out {\n        opacity: 0;\n      }\n      \n      .lds-circle > div {\n        display: inline-block;\n        width: 64px;\n        height: 64px;\n        margin: 8px;\n        border-radius: 50%;\n        background: silver;\n        animation: lds-circle 2.4s cubic-bezier(0, 0.2, 0.8, 1) infinite;\n      }\n\n      @keyframes lds-circle {\n        0%, 100% {\n          animation-timing-function: cubic-bezier(0.5, 0, 1, 0.5);\n        }\n        0% {\n          transform: rotateY(0deg);\n        }\n        50% {\n          transform: rotateY(1800deg);\n          animation-timing-function: cubic-bezier(0, 0.5, 0.5, 1);\n        }\n        100% {\n          transform: rotateY(3600deg);\n        }\n      }\n\n      /* CSS code for the overlay */\n      #overlay {\n        position: fixed;\n        top: 0;\n        left: 0;\n        width: 100%;\n        height: 100%;\n        background: #fff; /* white background */\n        z-index: 9998; /* make sure the overlay is behind the loader */\n        opacity: 1;\n        transition: opacity 0.5s ease-in-out;\n      }\n      \n      #overlay.fade-out {\n        opacity: 0;\n      }\n    </style>\n\n\n\n\n",
    "global_before_html": "\n\n",
    "global_footer_custom": "",
    "global_after_body": "<div id=\"loading-animation\">\n    <div id=\"overlay\"></div>\n    <div class=\"lds-circle\"><div></div></div>\n    </div>\n\n<script>\n      // Wait for the page to load\n      window.onload = function() {\n        // Get the loader and overlay elements\n        const loader = document.querySelector('.lds-circle');\n        const overlay = document.querySelector('#overlay');\n        \n        // Add the \"fade-out\" class to the loader and overlay elements\n        loader.classList.add('fade-out');\n        overlay.classList.add('fade-out');\n        \n        // Wait for the opacity transition to complete\n        setTimeout(function() {\n          // Remove the loader and overlay elements from the DOM\n          loader.parentNode.removeChild(loader);\n          overlay.parentNode.removeChild(overlay);\n        }, 500);\n      }\n    </script>",
